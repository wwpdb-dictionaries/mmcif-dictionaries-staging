## NDB_ENTITY_FUNC_BIND_MODE ##
###############################

save_ndb_entity_func_bind_mode
    _category.description
;              Data items in the NDB_ENTITY_FUNC_BIND_MODE category describe 
               characteristics of protein oligonucleotide binding.
;
    _category.id                  ndb_entity_func_bind_mode
    _category.mandatory_code      no
     loop_
    _category_key.name          '_ndb_entity_func_bind_mode.id'
                                '_ndb_entity_func_bind_mode.domain_id'
                                '_ndb_entity_func_bind_mode.entity_id'
     loop_
    _category_group.id           'inclusive_group'
                                 'entity_group'
                                 'ndb_group'
    _ndb_category_examples.case
;

 _ndb_entity_func_bind_mode.domain_id is:    ? 
 _ndb_entity_func_bind_mode.entity_id is:    2 
 _ndb_entity_func_bind_mode.id               1 
 _ndb_entity_func_bind_mode.protein_binds_to RNA 
 _ndb_entity_func_bind_mode.type             ENZYME 


;

     save_

save__ndb_entity_func_bind_mode.id
    _item_description.description
;              The value of _ndb_entity_func_bind_mode.id is a unique identifier
               for a binding mode within a domain within an entity.

               Note that this item need not be a number; it can be any unique
               identifier.
;
     loop_
    _item.name           
    _item.category_id          	
    _item.mandatory_code
     '_ndb_entity_func_bind_mode.id'                  ndb_entity_func_bind_mode    yes
     '_ndb_entity_func_enzyme.bind_mode_id'           ndb_entity_func_enzyme       yes
     '_ndb_entity_func_regulatory.bind_mode_id'       ndb_entity_func_regulatory   yes
     '_ndb_entity_func_structural.bind_mode_id'       ndb_entity_func_structural   yes
     '_ndb_entity_func_other.bind_mode_id'            ndb_entity_func_other        yes

     loop_
    _item_linked.child_name	
    _item_linked.parent_name    
     '_ndb_entity_func_enzyme.bind_mode_id'          '_ndb_entity_func_bind_mode.id'
     '_ndb_entity_func_regulatory.bind_mode_id'      '_ndb_entity_func_bind_mode.id'
     '_ndb_entity_func_structural.bind_mode_id'      '_ndb_entity_func_bind_mode.id'
     '_ndb_entity_func_other.bind_mode_id'           '_ndb_entity_func_bind_mode.id'
    _item_type.code               code
     save_

save__ndb_entity_func_bind_mode.domain_id
    _item_description.description
;              This data item is a pointer to _ndb_entity_poly_domain.id in the 
               NDB_ENTITY_POLY_DOMAIN category.
;
    _item.name                  '_ndb_entity_func_bind_mode.domain_id'
    _item.category_id             ndb_entity_func_bind_mode
    _item.mandatory_code          yes
    _item_type.code               code
     save_

save__ndb_entity_func_bind_mode.entity_id
    _item_description.description
;              This data item is a pointer to _entity.id in the ENTITY category.
;
    _item.name                  '_ndb_entity_func_bind_mode.entity_id'
    _item.category_id             ndb_entity_func_bind_mode
    _item.mandatory_code          yes
    _item_linked.child_name	'_ndb_entity_func_bind_mode.entity_id'
    _item_linked.parent_name    '_entity.id'
     save_

save__ndb_entity_func_bind_mode.protein_binds_to
    _item_description.description
;              This data item identifies the type of oligonucleotide to which
               the protein binds.
;
    _item.name                  '_ndb_entity_func_bind_mode.protein_binds_to'
    _item.category_id             ndb_entity_func_bind_mode
    _item.mandatory_code          yes
    _item_type.code               code
     loop_
    _item_enumeration.value
    _item_enumeration.detail      DNA          'Protein-DNA binding'
                                  RNA          'Protein-RNA binding'
     save_

save__ndb_entity_func_bind_mode.type
    _item_description.description
;              This data item describes the functional type of the 
               protein oligonucleotide binding interaction.
;
    _item.name                  '_ndb_entity_func_bind_mode.type'
    _item.category_id             ndb_entity_func_bind_mode
    _item.mandatory_code          yes
    _item_type.code               ucode
     loop_
    _item_enumeration.value
    _item_enumeration.detail      enzyme         'Enzymatic function'
                                  regulatory     'Regulatory function'
                                  structural     'Structural function'
                                  other          'Other function'
     save_

