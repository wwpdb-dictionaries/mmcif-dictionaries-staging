 _rcsb_nmr_force_constants.exptl_J_coupling_term_units                   'kcal/mol/Hz**2'  
 _rcsb_nmr_force_constants.exptl_13C_shift_term                           0.5 
 _rcsb_nmr_force_constants.exptl_13C_shift_term_units                    'kcal/mol/ppm**2' 
 _rcsb_nmr_force_constants.exptl_1H_shift_term                            7.5 
 _rcsb_nmr_force_constants.exptl_1H_shift_term_units                     'kcal/mol/ppm**2'
 _rcsb_nmr_force_constants.covalent_geom_bond_term                        1000. 
 _rcsb_nmr_force_constants.covalent_geom_bond_term_units                  'kcal/mol/A**2'
 _rcsb_nmr_force_constants.non-bonded_inter_van_der_Waals_term_type       4.
 _rcsb_nmr_force_constants.non-bonded_inter_van_der_Waals_term_units     'kcal/mol/A**4'

;
save_


