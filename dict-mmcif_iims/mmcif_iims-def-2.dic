    _item.category_id            em_entity_assembly
    _item.mandatory_code         no
    _item_type.code              line
     loop_
    _item_enumeration.value
        VIRUS
        PROTEIN
        'NUCLEIC ACID'
        LIGAND
        LABEL
        'CELLULAR COMPONENT'
        RIBOSOME
        TISSUE
        CELL
        'ORGANELLE OR CELLULAR COMPONENT'
        COMPLEX
    save_

save__em_entity_assembly.name
